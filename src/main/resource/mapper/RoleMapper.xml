<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.jsonXxxx.jyTest.mapper.RoleMapper">
	<insert id="insertOne" useGeneratedKeys="true" keyProperty="roleId"
		parameterType="cn.jsonXxxx.jyTest.entity.Role">
		insert into role(role_name,remark,dept_id,create_time)
		values(#{roleName},#{remark},#{deptId},#{createTime})
	</insert>
</mapper>
